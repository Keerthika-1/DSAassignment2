		int missingNum = findMissingNum(a);
		System.out.println(missingNum);

		}

	private static int findMissingNum(int[] a) {

		int start = 0;
		int end = a.length-1;

		while(start<=end)
		{
			int mid = start + (end-start)/2;

			if(mid == 0)
			{
				return mid;
			}


			if(a[mid]!=mid && a[mid-1]==mid-1)
			{
				return mid;
			}
				else if(a[mid]==mid)
			{
				start = mid +1;
			}


			else
			{
				end = mid -1;
			}
		}
		return a.length;
	}
  
  
