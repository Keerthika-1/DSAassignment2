	int[]a = {3,6,8}  ;
	int[] b={1,2,3,5,6,90,789};
		for(int i=0;i<a.length;i++) {
			for(int j=0;j<b.length;j++) {
				if(a[i]==b[j]) {
					System.out.println(a[i]);
					}
				}
			}
		}
	}
